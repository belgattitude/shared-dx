    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.17",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.1.1",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-mdx": "3.1.5",
