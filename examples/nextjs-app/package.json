    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "autoprefixer": "10.4.21",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.1.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-storybook": "9.0.16",
