    "@httpx/memo-intl": "1.4.8",
    "@httpx/plain-object": "2.1.7",
    "@httpx/treeu": "0.5.7",
    "@next/mdx": "16.0.3",
    "@tanstack/query-core": "5.90.12",
    "@tanstack/react-query": "5.90.12",
    "@vercel/og": "0.8.5",
    "clsx": "2.1.1",
    "graphql": "16.12.0",
    "ky": "1.14.1",
    "mdx": "0.3.1",
    "next": "16.0.3",
    "next-secure-headers": "2.2.0",
    "picocolors": "1.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.5",
    "tailwind-merge": "3.4.0",
    "zod": "4.1.13",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@next/bundle-analyzer": "16.0.3",
    "@storybook/addon-docs": "9.1.15",
