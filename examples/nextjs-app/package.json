    "mdx": "0.3.1",
    "next": "15.3.5",
    "next-secure-headers": "2.2.0",
    "picocolors": "1.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.3",
    "tailwind-merge": "2.6.0",
    "zod": "4.0.5",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
