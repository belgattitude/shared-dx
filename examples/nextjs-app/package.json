    "@next/mdx": "14.1.0",
    "@nextvalid/zod-request": "0.4.0",
    "@tanstack/query-core": "5.20.2",
    "@tanstack/react-query": "5.20.2",
    "@vercel/og": "0.6.2",
    "clsx": "2.1.0",
    "graphql": "16.8.1",
