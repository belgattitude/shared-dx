    "picocolors": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "sharp": "0.32.4",
    "tailwind-merge": "1.14.0",
    "zod": "3.21.4",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
