    "mdx": "0.3.1",
    "next": "14.1.0",
    "next-secure-headers": "2.2.0",
    "picocolors": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "sharp": "0.33.2",
    "tailwind-merge": "2.2.1",
    "zod": "3.22.4",
    "zustand": "4.5.0"
