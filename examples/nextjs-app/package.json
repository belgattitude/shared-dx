    "@httpx/plain-object": "2.1.0",
    "@httpx/treeu": "0.5.0",
    "@next/mdx": "15.3.5",
    "@tanstack/query-core": "5.83.0",
    "@tanstack/react-query": "5.83.0",
    "@vercel/og": "0.7.2",
    "clsx": "2.1.1",
    "graphql": "16.11.0",
